public class Recursion {
	static int  a = 1;
	static int count = 0;
	public  int sum() {
		count = count +a;
		a++;	
		return
			count;
	}
	public static void main(String[] args) {
		while ( a<10 ) {
			new Practice1().sum();
			System.out.println(count);
		};
		System.out.println(count);
	}
}
//递归  1加至9
