public class Bubble {
	public static void main(String[] args) {
		int temp = 0;
		int[] test = {1,4,5,13,54,0,2};
		for ( int i = 0; i<test.length; i++) {
			for ( int j = 0; j<test.length-1-i; j++) {
				if( test[j]>test[j+1]) {
					temp =test[j]  ;													
					test[j] = test[j+1];
					test[j+1] = temp;
				}
			}
		}
		for ( int i = 0; i<test.length; i++) {
			System.out.println( test[i]);
		}
		}
	}
